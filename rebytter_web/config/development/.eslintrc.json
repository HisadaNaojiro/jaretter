{
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "es6": true,
    "mocha": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "jsx-a11y/anchor-is-valid": [ "error", {
      "components": [ "Link" ],
      "specialLink": [ "to" ]
    }],
    "arrow-body-style": ["error", "always"],
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "react/prop-types": 0
  },
  "globals": { "document": true },
  "extends": "airbnb"
}
